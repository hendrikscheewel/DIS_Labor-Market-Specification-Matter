number = {6},
pages = {1341--1385},
title = {{Immigration and voting for the far right}},
volume = {15},
year = {2017}
}
@article{Ortega2014,
abstract = {Two large but separate bodies of literature analyze the economic effects of international trade and immigration. Given that several factors are important determinants of both trade and migration flows, the previous studies are vulnerable to a potentially serious omitted-variables bias, questioning the validity of existing estimates of the effects of trade and immigration on income. This paper provides estimates of the effects of trade and immigration on income in a unified framework. We also provide a useful decomposition of the channels at work in terms of the employment rate, the capital intensity, and total factor productivity of the receiving economy. We assemble panel data on immigration flows, output, employment and capital stocks for thirty OECD countries over the period 1980-2007. In order to identify the causal effects of trade and immigration on economic outcomes we adopt and extend the gravity-based approach in Frankel and Romer (1999). Our predictors for trade and immigration flows are based on geography and the demographic trends of each country's trade and migration partners. We find that immigration has a large, positive effect on the employment rate of the receiving country. However, it leaves income per capita unaffected because of an offsetting negative effect on TFP. In contrast, trade flows appear to increase income per capita, mainly through TFP growth, and have no impact on the employment rate. The positive employment effect of immigration is the most robust of all the effects identified in this paper.},
author = {Ortega, Francesc and Peri, Giovanni},
doi = {10.1007/978-3-319-04078-3_2},
file = {:home/h/Shared/literature/Ortega, Peri - 2014 - Unknown - The Aggregate Effects of Trade and Migration Evidence from OECD Countries.pdf:pdf},
keywords = {DIS paper,geography,income,international migration,trade},
mendeley-tags = {DIS paper},
number = {5604},
pages = {19--51},
title = {{The Aggregate Effects of Trade and Migration: Evidence from OECD Countries}},
url = {http://link.springer.com/10.1007/978-3-319-04078-3{\_}2},
year = {2014}
}
@article{Manacorda2012,
abstract = {Immigration to the UK has risen over time. Existing studies of the impact of$\backslash$r$\backslash$nimmigration on the wages of native-born workers in the UK have failed to find any$\backslash$r$\backslash$nsignificant effect. This is something of a puzzle since Card and Lemieux, (2001) have$\backslash$r$\backslash$nshown that changes in the relative supply of educated natives do seem to have$\backslash$r$\backslash$nmeasurable effects on the wage structure. This paper offers a resolution of this$\backslash$r$\backslash$npuzzle – natives and immigrants are imperfect substitutes, so that an increase in$\backslash$r$\backslash$nimmigration reduces the wages of immigrants relative to natives. We show this using$\backslash$r$\backslash$na pooled time series of British cross-sectional micro data of observations on male$\backslash$r$\backslash$nwages and employment from the mid-1970s to the mid-2000s. This lack of$\backslash$r$\backslash$nsubstitution also means that there is little discernable effect of increased immigration$\backslash$r$\backslash$non the wages of native-born workers, but that the only sizeable effect of increased$\backslash$r$\backslash$nimmigration is on the wages of those immigrants who are already here.},
author = {Manacorda, Marco and Manning, Alan and Wadsworth, Jonathan},
doi = {10.1111/j.1542-4774.2011.01049.x},
file = {:home/h/Shared/literature/Manacorda, Manning, Wadsworth - 2012 - Journal of the European Economic Association - The impact of immigration on the structure of wage.pdf:pdf},
issn = {15424766},
journal = {Journal of the European Economic Association},
keywords = {DIS paper},
mendeley-tags = {DIS paper},
number = {1},
pages = {120--151},
title = {{The impact of immigration on the structure of wages: Theory and evidence from britain}},
volume = {10},
year = {2012}
}

@article{Battisti2014,
abstract = {We study the effects of immigration on native welfare in a general equilibrium model featuring two skill types, search frictions, wage bargaining, and a redistributive welfare state. Our quantitative analysis suggests that, in all 20 countries studied, immigration attenuates the effects of search frictions. These gains tend to outweigh the welfare costs of redistribution. Immigration has increased native welfare in almost all countries. Both high-skilled and low-skilled natives benefit in two thirds of countries, contrary to what models without search frictions predict. Median total gains from migration are 1.19{\%} and 1.00{\%} for high and low skilled natives, respectively.},
annote = {Contribution: 
First paper that takes into account both imperfect competition in the labor market and redistribution.

Shortcomings:
- disregard of participation rates
- no age groups
- no VAT tax
- no redistributive transfers

Model features:
- General equilibrium
- Skill heterogeneity: low/high skilled
- Search frictions
- Wage bargaining
- Redistributive welfare state
- Capital in production function

Countries: 20 OECD countries

4 Welfare effect channels of immigration:
1. labour market:
1.1 complementarity effect on wages
1.2 job-creation effect
2. redistribution:
2.1 unemployment benefits
2.2 proportional taxes and lump-sum transfers

Results:
- important determinants of welfare effect: wage gaps and unemployment gaps

Difference with respect to Chassamboulli (2014):
- less theoretical: quantitative analysis for a large sample of countries
- matches with immigrants with different expected durations
- welfare state},
author = {Battisti, Michele and Felbermayr, Gabriel and Peri, Giovanni and Poutvaara, Panu},
doi = {10.3386/w20131},
file = {:home/h/Shared/literature/Battisti et al. - 2014 - NBER Working Papers 20131 - Immigration, Search, and Redistribution A Quantitative Assessment of Native Welfare.pdf:pdf},
journal = {NBER Working Papers 20131},
keywords = {DIS paper,Labor Studies,Public Economics},
mendeley-tags = {DIS paper},
title = {{Immigration, Search, and Redistribution: A Quantitative Assessment of Native Welfare}},
url = {http://ideas.repec.org/p/nbr/nberwo/20131.html},
year = {2014}
}
@incollection{OECD2013,
abstract = {The net fiscal impact of immigration is typically estimated as the difference between the taxes and other contributions migrants make to public finances and the costs of the public benefits and services they receive. This impact depends on the characteristics of migrants, their impacts on the labour market and the characteristics and rules of the welfare system, among other factors. In theory, migrants who are young, skilled and doing highly-paid jobs are likely to make a more positive net fiscal contribution than those with low skills and low labour market participation rates. The evidence suggests that the fiscal impact of migration in the UK is small (less than +/-1{\%} of GDP) and differs by migrant group (e.g. EEA migrants vs non-EEA migrants, recent migrants vs all migrants). The existing results are subject to numerous key assumptions. The Office for Budget Responsibility suggests higher net migration reduces pressure on government debt over time. This result is based on the assumption that incoming migrants are more likely to be of working age than the population in general. Cross-country evidence for the years 2007-2009 suggests that the fiscal impact of migration in the UK (+ 0.46{\%} of GDP) was greater than the fiscal impact of migration in 16 other OECD countries. The UK occupies the 11th position (out of 27 countries) in regards to the positive fiscal impact of migration across OECD countries. This briefing provides an overview of the existing estimates on the impact of immigration on government finances for the UK and other countries and explores the conceptual and methodological issues related to estimating the fiscal impact of immigration.},
author = {OECD},
booktitle = {International Migration Outlook},
doi = {10.1787/migr_outlook-2013-6-en},
file = {:home/h/Shared/literature/OECD - 2013 - International Migration Outlook - The fiscal impact of immigration in OECD countries.pdf:pdf},
keywords = {DIS paper},
mendeley-tags = {DIS paper},
pages = {125--190},
title = {{The fiscal impact of immigration in OECD countries}},
year = {2013}
}
@article{Brucker2011,
abstract = {In this paper we employ a wage-setting approach to analyze the labor market effects of immigration into Germany from 1980 to 2004. This enables us to consider labor market rigidities, which are prevalent in Europe. We find that the elasticity of the wage-setting curve is particularly high for young workers. Moreover, natives and foreigners are imperfect substitutes. The wage and employment effects of immigration depend on the skill structure of the immigrant workforce. Because the foreign labor supply shift has mainly affected the high- skilled labor market segment, the 4 percent increase of the workforce through immigration has not increased either aggregate or foreign unemployment.},
