@article{Chojnicki2011,
abstract = {This paper examines the economic impact of the second great immigration wave (1945–2000) on the US economy. Our analysis relies on a computable general equilibrium model combining the major interactions between immigrants and natives (labor market impact, fiscal impact, capital deepening, endogenous education, endogenous inequality). Contrary to recent studies, we show that immigration induced important net gains and small redistributive effects among natives. According to our simulations, the postwar US immigration is beneficial for all natives cohorts and all skill groups. Nevertheless, the gains would have been larger if the US had conducted a more selective immigration policy.},
author = {Chojnicki, Xavier and Docquier, Fr{\'{e}}d{\'{e}}ric and Ragot, Lionel},
doi = {10.1007/s00148-009-0286-z},
file = {:home/h/Shared/literature/Chojnicki, Docquier, Ragot - 2011 - Journal of Population Economics - Should the US have locked heaven's door Reassessing the benefits o.pdf:pdf},
issn = {09331433},
journal = {Journal of Population Economics},
keywords = {Computable general equilibrium,DIS paper,Immigration,Welfare,fiscal effects},
mendeley-tags = {DIS paper,fiscal effects},
number = {1},
pages = {317--359},
title = {{Should the US have locked heaven's door?: Reassessing the benefits of postwar immigration}},
url = {https://ideas.repec.org/a/spr/jopoec/v24y2011i1p317-359.html https://link.springer.com/content/pdf/10.1007{\%}2Fs00148-009-0286-z.pdf http://link.springer.com/10.1007/s00148-009-0286-z},
volume = {24},
year = {2011}
}
@article{Auerbach1999,
abstract = {This article reconsiders the fiscal impact of immigrants over time, using the technique of generational accounting introduced by Auerbach et al. (1991) and applied subsequently by a number of others. The authors concluded: first, whether immigration contributes to or helps alleviate fiscal stress depends on the extent to which that stress will be borne by future generations. Second, when a policy of 'fiscal responsibility' is followed, the fiscal gain from immigration is reduced. Third, the impact of immigration on fiscal balances is extremely small relative to the size of the overall imbalance itself. Thus, immigration should be viewed neither as a major source of the existing imbalance nor as a potential solution to it.},
author = {Auerbach, Alan J. and Oreopoulos, Philip},
doi = {10.1257/aer.89.2.176},
file = {:home/h/Shared/literature/Auerbach, Oreopoulos - 1999 - American Economic Review - Analyzing the fiscal impact of U.S. immigration.pdf:pdf},
