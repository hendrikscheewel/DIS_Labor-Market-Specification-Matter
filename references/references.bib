@article{Card2009,
abstract = {Immigration is often viewed as a proximate cause of the rising wage gap between highand$\backslash$r$\backslash$nlow-skilled workers. Nevertheless, there is controversy over the appropriate$\backslash$r$\backslash$ntheoretical and empirical framework for measuring the presumed effect, and over the$\backslash$r$\backslash$nprecise magnitudes involved. This paper offers an overview and synthesis of existing$\backslash$r$\backslash$nknowledge on the relationship between immigration and inequality, focusing on evidence$\backslash$r$\backslash$nfrom cross-city comparisons in the U.S. While some researchers have claimed that a$\backslash$r$\backslash$ncross-city research design is inherently flawed, I argue that the evidence from cross-city$\backslash$r$\backslash$ncomparisons is remarkably consistent with recent findings based on aggregate time$\backslash$r$\backslash$nseries data. In particular, cross-city and aggregate time series comparisons provide$\backslash$r$\backslash$nsupport for three key conclusions: (1) workers with below high school education are$\backslash$r$\backslash$nperfect substitutes for those with a high school education; (2) “high school equivalent”$\backslash$r$\backslash$nand “college equivalent” workers are imperfect substitutes, with an elasticity of$\backslash$r$\backslash$nsubstitution on the order of 2; (3) within education groups, immigrants and natives are$\backslash$r$\backslash$nimperfect substitutes. Together these results imply that the average impacts of recent$\backslash$r$\backslash$nimmigrant inflows on the relative wages of U.S. natives are small. The effects on overall$\backslash$r$\backslash$nwage inequality (including natives and immigrants) are larger, reflecting the$\backslash$r$\backslash$nconcentration of immigrants in the tails of the skill distribution and higher residual$\backslash$r$\backslash$ninequality among immigrants than natives. Even so, immigration accounts for a small$\backslash$r$\backslash$nshare (5{\%}) of the increase in U.S. wage inequality between 1980 and 2000.},
archivePrefix = {arXiv},
arxivId = {arXiv:1011.1669v3},
author = {Card, David},
doi = {10.1257/aer.99.2.1},
