journal = {European Economic Review},
keywords = {Comparative studies,DIS paper,Immigration,Panel data,Unemployment,Wages},
mendeley-tags = {DIS paper},
pages = {205--225},
publisher = {Elsevier},
title = {{Migration and imperfect labor markets: Theory and cross-country evidence from Denmark, Germany and the UK}},
url = {http://dx.doi.org/10.1016/j.euroecorev.2013.11.007 http://linkinghub.elsevier.com/retrieve/pii/S0014292113001438},
volume = {66},
year = {2014}
}
